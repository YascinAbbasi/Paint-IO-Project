<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="625.0" prefWidth="625.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.paintioproject.GameModeController">
   <children>
      <ImageView fx:id="GameModeImageView" fitHeight="625.0" fitWidth="625.0" layoutX="-14.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true" />
      <Label alignment="CENTER" layoutY="34.0" prefHeight="21.0" prefWidth="625.0" text="GameMode">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
      <Button fx:id="BackButton" layoutX="584.0" layoutY="22.0" mnemonicParsing="false" onAction="#BackButton" prefHeight="17.0" prefWidth="27.0" text="←" />
      <Button layoutX="247.0" layoutY="517.0" mnemonicParsing="false" onAction="#StartButton" prefHeight="21.0" prefWidth="132.0" text="Start!" />
      <ChoiceBox fx:id="ColorChoiceBox" layoutX="14.0" layoutY="161.0" prefWidth="150.0" />
      <Label fx:id="ErrorLabel" alignment="CENTER" layoutY="72.0" prefHeight="17.0" prefWidth="625.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="135.0" prefHeight="17.0" prefWidth="150.0" text="pick your Color :">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="SpeedChoiceBox" layoutX="417.0" layoutY="161.0" prefHeight="25.0" prefWidth="165.0" />
      <Label layoutX="417.0" layoutY="135.0" text="Select your Desired Speed:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="DifficultyLevelChoiceBox" layoutX="14.0" layoutY="320.0" prefHeight="25.0" prefWidth="169.0" />
      <ChoiceBox fx:id="BotNumberChoiceBox" layoutX="412.0" layoutY="320.0" prefHeight="25.0" prefWidth="175.0" />
      <Label layoutX="14.0" layoutY="299.0" text="Select your Difficulty Level :">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="424.0" layoutY="301.0" />
      <Label layoutX="412.0" layoutY="300.0" text="Choose the Number of Bots:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
